{
  "name": "Getting Started Example",
  "options_page": "/app/views/options.html",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "storage", "declarativeContent"],
  "background": {
    "scripts": ["app/assets/javascripts/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "/app/views/popup.html",
    "default_icon": {
      "16": "app/assets/images/get_started16.png",
      "32": "app/assets/images/get_started32.png",
      "48": "app/assets/images/get_started48.png",
      "128": "app/assets/images/get_started128.png"
    }
  },
  "icons": {
    "16": "app/assets/images/get_started16.png",
    "32": "app/assets/images/get_started32.png",
    "48": "app/assets/images/get_started48.png",
    "128": "app/assets/images/get_started128.png"
  },
  "manifest_version": 2
}